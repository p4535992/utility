<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- doc_id,url,city,latitude,longitude,crawling_date,crawling_time; -->
<hibernate-mapping package="object.model">
  <class name="object.model.Document" table="document">
    <id column="doc_id" name="doc_id" type="int">
      <generator class="increment"/>
    </id>
    <property name="url">
      <column name="url"/>
    </property>
    <property name="city">
      <column name="city"/>
    </property>
    <property name="latitude">
      <column name="latitude"/>
    </property>
    <property name="longitude">
      <column name="longitude"/>
    </property>
    <property name="crawling_date">
      <column name="crawling_date"/>
    </property>
    <property name="crawling_time">
      <column name="crawling_time"/>
    </property>
  </class>
</hibernate-mapping>
