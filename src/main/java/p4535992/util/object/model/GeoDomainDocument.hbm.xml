<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="object.model">
  <class name="object.model.GeoDomainDocument" table="geodomaindocument_h">
    <id column="doc_id" name="doc_id" type="int">
      <generator class="increment"/>
    </id>
    <property name="url" type="url">
      <column name="url"/>
    </property>
    <property name="regione" type="string">
      <column name="regione"/>
    </property>
    <property name="provincia" type="string">
      <column name="provincia"/>
    </property>
    <property name="city" type="string">
      <column name="city"/>
    </property>
    <property name="indirizzo" type="string">
      <column name="indirizzo"/>
    </property>
    <property name="iva" type="string">
      <column name="iva"/>
    </property>
    <property name="email" type="string">
      <column name="email"/>
    </property>
    <property name="telefono" type="string">
      <column name="telefono"/>
    </property>
    <property name="fax" type="string">
      <column name="fax"/>
    </property>
    <property name="edificio" type="string">
      <column name="edificio"/>
    </property>
    <property name="lat" type="double">
      <column name="latitude"/>
    </property>
    <property name="lng" type="double">
      <column name="longitude"/>
    </property>
    <property name="nazione" type="string">
      <column name="nazione"/>
    </property>
    <property name="description" type="string">
      <column name="description"/>
    </property>
    <property name="indirizzoNoCAP" type="string">
      <column name="indirizzoNoCAP"/>
    </property>
    <property name="postalCode" type="string">
      <column name="postalCode"/>
    </property>
    <property name="indirizzoHasNumber" type="string">
      <column name="indirizzoHasNumber"/>
    </property>
  </class>
</hibernate-mapping>
